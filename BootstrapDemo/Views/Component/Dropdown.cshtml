@section Scripts{
    <script>
        $(document).ready(function () {
            $('[data-submenu]').submenupicker();

            $('.expandButton').on('click', (e) => {
                $('.dropdown-menu').collapse('show')
            });
            $('.exchangeButton').on('click', (e) => {
                $('.dropdown-menu').collapse('toggle')
            });
            $('.compressButton').on('click', (e) => {
                $('.dropdown-menu').collapse('hide')
            });
            $('.markerButton').on('click', (e) => {
                $('.dropdown-menu').collapse('update')
            });

            $('#rightClickContextMenuContainer')
                .on('contextmenu', function (e) {
                    var top = e.pageY;
                    var left = e.pageX;
                    $("#rightClickContextMenu")
                        .css({
                            display: "block",
                            top: top,
                            left: left
                        })
                        .addClass("show")
                        .on("blur click touchend", function () {
                            $(this).removeClass("show").hide();
                        })
                        .focus()
                        .attr('tabIndex', -1);
                    //blocks browser default menu
                    return false;
                });
        })
    </script>
}